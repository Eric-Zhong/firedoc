

<a name="index" class="anchor-link"></a>
<div class="index">
    <h2>索引</h2>
    {{>options}}

{{#if properties}}
<div class="index-section properties">
    <h3>属性(Property)</h3>
    <ul class="index-list properties">
        {{#properties}}
        <li class="index-item property{{#if access}} {{access}}{{/if}}{{#if deprecated}} deprecated{{/if}}{{#if overwritten_from}} inherited{{/if}}">
            <a href="#property_{{name}}">{{name}}</a>
            {{#if static}}
                <span class="flag static">静态</span>
            {{/if}}
        </li>
        {{/properties}}
    </ul>
</div>
{{/if}}

{{#if attrs}}
<div class="index-section attrs">
    <h3>属性(Attribute)</h3>

    <ul class="index-list attrs">
        {{#attrs}}
        <li class="index-item attr{{#if access}} {{access}}{{/if}}{{#if deprecated}} deprecated{{/if}}{{#if overwritten_from}} inherited{{/if}}">
            <a href="#attr_{{name}}">{{name}}</a>
        </li>
        {{/attrs}}
    </ul>
</div>
{{/if}}

{{#if methods}}
<div class="index-section methods">
    <h3>方法</h3>
    <ul class="index-list methods">
        {{#is_constructor}}
        <li class="index-item method{{#if access}} {{access}}{{/if}}{{#if deprecated}} deprecated{{/if}} constructor">
            <a href="#method_{{name}}">{{name}}</a>
            <span class="flag constructor">constructor</span>
            {{#if static}}
                <span class="flag static">静态</span>
            {{/if}}
            {{#if deprecated}}
                <span class="flag deprecated">已废弃</span>
            {{/if}}
        </li>
        {{/is_constructor}}

        {{#methods}}
        <li class="index-item method{{#if access}} {{access}}{{/if}}{{#if deprecated}} deprecated{{/if}}{{#if overwritten_from}} inherited{{/if}}">
            <a href="#method_{{name}}">{{name}}</a>
            {{#if static}}
                <span class="flag static">静态</span>
            {{/if}}
            {{#if deprecated}}
                <span class="flag deprecated">已废弃</span>
            {{/if}}
        </li>
        {{/methods}}
    </ul>
</div>
{{/if}}

{{#if events}}
<div class="index-section events">
    <h3>事件</h3>
    <ul class="index-list events">
        {{#events}}
        <li class="index-item event{{#if access}} {{access}}{{/if}}{{#if deprecated}} deprecated{{/if}}{{#if overwritten_from}} inherited{{/if}}">
            <a href="#event_{{name}}">{{name}}</a>
            {{#if static}}
                <span class="flag static">静态</span>
            {{/if}}
            {{#if deprecated}}
                <span class="flag deprecated">已废弃</span>
            {{/if}}
        </li>
        {{/events}}
    </ul>
</div>
{{/if}}

{{#if extends}}
    <div class="index-section inherits-members">
        <h3>继承成员变量</h3>
        
        <h4>属性(Property)</h4>
        <ul class="index-list properties">
        {{#inheritedItems.properties}}
            <li class="index-item property{{#if access}} {{access}}{{/if}}{{#if deprecated}} deprecated{{/if}}">
                <a href="{{extended_from}}.html#property_{{name}}">{{name}}</a>
            </li>
        {{/inheritedItems.properties}}
        </ul>
        
        <h4>属性(Attribute)</h4>
        <ul class="index-list attrs">
        {{#inheritedItems.attrs}}
            <li class="index-item attr{{#if access}} {{access}}{{/if}}{{#if deprecated}} deprecated{{/if}}">
                <a href="{{extended_from}}.html#attr_{{name}}">{{name}}</a>
            </li>
        {{/inheritedItems.attrs}}
        </ul>

        <h4>方法</h4>
        <ul class="index-list methods">
        {{#inheritedItems.methods}}
            <li class="index-item method{{#if access}} {{access}}{{/if}}{{#if deprecated}} deprecated{{/if}}">
                <a href="{{extended_from}}.html#method_{{name}}">{{name}}</a>
            </li>
        {{/inheritedItems.methods}}
        </ul>

        <h4>事件</h4>
        <ul class="index-list events">
        {{#inheritedItems.events}}
            <li class="index-item event{{#if access}} {{access}}{{/if}}{{#if deprecated}} deprecated{{/if}}">
                <a href="{{extended_from}}.html#event_{{name}}">{{name}}</a>
            </li>
        {{/inheritedItems.events}}
        </ul>

    </div>
{{/if}}

{{#if children}}
    <div class="index-section inherits-tree">
        <h3>继承树</h3>
        {{> class-tree}}
    </div>
{{/if}}

</div>